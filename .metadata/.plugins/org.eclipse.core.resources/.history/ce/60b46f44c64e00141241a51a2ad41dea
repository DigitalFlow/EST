package Schule.Kram;

public class Girokonto extends Konto {
	public double dispot;
	
	public Girokonto() {
		dispot = 0;
	}
	
	public void setDispot(double value) {
		dispot = value;
	}
	
	public double getDispot() 
	{
		return dispot;
	}
	
	public void print()
	{
		System.out.println("Kontonummer: " + getkNr() + ", Kontostand: " + getkStand() + ", Dispo: " + getDispot());
	}
}
